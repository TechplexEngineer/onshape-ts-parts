{
    "compilerOptions": {
        "target": "ES2017",
        "module": "es2020",
        "lib": [
            "es6",
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "declaration": true,
        "outDir": "lib",
        "strict": false,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "test/"
    ]
}